  RowBox[{
   SubscriptBox["\[Phi]", "0"], "[", 
   RowBox[{"x", ",", "y"}], "]"}], "/.", 
  RowBox[{"{", 
   RowBox[{
    RowBox[{"f", "\[Rule]", 
     RowBox[{"(", 
      RowBox[{"#", "&"}], ")"}]}], ",", 
    RowBox[{"g", "\[Rule]", 
     RowBox[{"(", 
      RowBox[{
       SuperscriptBox["#", "2"], "&"}], ")"}]}]}], "}"}]}]], "Input",
 CellChangeTimes->{{3.768200459817437*^9, 3.768200486024519*^9}, {
  3.7682006948366013`*^9, 3.768200706807206*^9}, {3.7682007833346233`*^9, 
  3.7682008602930517`*^9}, {3.768200894216585*^9, 3.7682008949718237`*^9}, {

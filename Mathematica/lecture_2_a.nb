 CellLabel->"In[31]:=",ExpressionUUID->"246df315-a279-4eed-94ab-c9b01c9f1c19"],

Cell[BoxData[
 RowBox[{
  FractionBox["1", "2"], " ", 
  RowBox[{"Log", "[", 
   FractionBox[
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{"1", "-", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"x", "-", 
          RowBox[{"\[ImaginaryI]", " ", "y"}]}], ")"}], " ", 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"x", "+", 
           RowBox[{"\[ImaginaryI]", " ", "y"}]}], ")"}], "2"]}]}], ")"}], 
     "2"], 
    RowBox[{"8", " ", 
     RowBox[{"(", 
      RowBox[{"x", "+", 
       RowBox[{"\[ImaginaryI]", " ", "y"}]}], ")"}]}]], "]"}]}]], "Output",
 CellChangeTimes->{3.7682008613742113`*^9, 3.768200895730015*^9, 
